<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpErrors>
            <remove statusCode="502" subStatusCode="-1" />
            <remove statusCode="501" subStatusCode="-1" />
            <remove statusCode="500" subStatusCode="-1" />
            <remove statusCode="412" subStatusCode="-1" />
            <remove statusCode="406" subStatusCode="-1" />
            <remove statusCode="405" subStatusCode="-1" />
            <remove statusCode="404" subStatusCode="-1" />
            <remove statusCode="403" subStatusCode="-1" />
            <remove statusCode="401" subStatusCode="-1" />
            <error statusCode="400" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\bad_request.html" />
            <error statusCode="407" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\proxy_authentication_required.html" />
            <error statusCode="414" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\request-uri_too_long.html" />
            <error statusCode="415" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\unsupported_media_type.html" />
            <error statusCode="503" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\maintenance.html" />
            <error statusCode="401" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\unauthorized.html" />
            <error statusCode="403" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\forbidden.html" />
            <error statusCode="404" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\not_found.html" />
            <error statusCode="405" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\method_not_allowed.html" />
            <error statusCode="406" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\not_acceptable.html" />
            <error statusCode="412" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\precondition_failed.html" />
            <error statusCode="500" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\internal_server_error.html" />
            <error statusCode="501" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\not_implemented.html" />
            <error statusCode="502" prefixLanguageFilePath="" path="D:\Inetpub\vhosts\woxsen.edu.in\error_docs\bad_gateway.html" />
        </httpErrors>
        <rewrite>
		  <rules>
			<rule name="ReactRouter Routes" stopProcessing="true">
			  <match url=".*" />
			  <conditions logicalGrouping="MatchAll">
				<add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
			  </conditions>
			  <action type="Rewrite" url="index.html" />
			</rule>

		  </rules>
		</rewrite>
    </system.webServer>
</configuration>
